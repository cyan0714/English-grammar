{
  "title": "English Grammar Learning Guide - 英语语法学习指南",
  "author": "English Grammar Team",
  "description": "系统性的英语语法学习资源库，适合中文母语者学习英语语法",
  "language": "zh",
  "gitbook": "3.2.3",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "search-pro",
    "anchor-navigation-ex",
    "expandable-chapters",
    "back-to-top-button",
    "chapter-fold",
    "code",
    "splitter",
    "tbfed-pagefooter",
    "theme-comscore",
    "quiz",
    "katex",
    "popup",
    "toggle-chapters",
    "custom-favicon",
    "emphasize",
    "accordion"
  ],
  "pluginsConfig": {
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": true,
      "printLog": false,
      "multipleH1": true,
      "mode": "float",
      "showGoTop": true,
      "float": {
        "floatIcon": "fa fa-navicon",
        "showLevelIcon": false,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right"
      }
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy; 2024 English Grammar Learning Guide",
      "modify_label": "最后修改时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "theme-comscore": {
      "nav": [
        {
          "url": "https://github.com/your-username/English-grammar",
          "target": "_blank",
          "name": "GitHub"
        }
      ]
    },
    "emphasize": {
      "type": "yellow"
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}
